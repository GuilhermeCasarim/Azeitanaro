<div class="new-user">
  <div class="acc-forms">
    <div class="form-label">
      <h1>Editar Perfil</h1>
    </div>

    <div class="form-inputs">
      <%= form_for current_user do |f| %>
        <%= render 'fields', f: f %>

        <%= f.submit 'Atualizar', class: 'btn btn-primary submit-button' %>
      <% end %>
    </div>
  </div>
</div>